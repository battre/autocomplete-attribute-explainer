      control_type: INPUT
      # concepts: "floor"
      # concepts: "apartment"
      concepts: "location-in-building"
      section: ADDRESS
    }
    fields {
      name: "|referencePoint"
      label: "Información referencia"
      label_translated: "Reference information"
