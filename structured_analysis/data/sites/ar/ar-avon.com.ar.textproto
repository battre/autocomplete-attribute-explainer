site_examples {
  url: "https://www.avon.com.ar"
  is_structured: true
  locale {
    country: "AR"
    language: "es"
  }
  sequences {
    fields {
      name: "react-select-2-input|"
      label: "Provincia"
      label_translated: "Province"
      control_type: INPUT
      concepts: "address-level1"
      section: ADDRESS
    }
    fields {
      name: "react-select-3-input|"
      label: "Localidad"
      label_translated: "Location"
      control_type: INPUT
      concepts: "address-level1"
      autocomplete_attribute: "none"
      section: ADDRESS
    }
