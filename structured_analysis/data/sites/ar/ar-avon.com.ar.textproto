      name: "|neighborhood"
      label: "Municipio"
      label_translated: "Municipality"
      control_type: INPUT
      concepts: "address-level3"
      section: ADDRESS
    }
    fields {
      name: "|postalCode"
      label: "Código postal"
      label_translated: "Postal Code"
      example: "B3643BAC"
      example_translated: "B3643BAC"
      control_type: INPUT
      concepts: "postal-code"
      section: ADDRESS
    }
    fields {
      name: "autocomplete-input|address"
      label: "Calle"
