      example_translated: "Email or ID"
      control_type: INPUT
      concepts: "unset"
      section: LOGIN
    }
    fields {
      name: "|password"
      label: "Contraseña"
      label_translated: "Password"
      example: "Contraseña"
      example_translated: "Password"
      control_type: INPUT
      concepts: "unset"
      section: LOGIN
