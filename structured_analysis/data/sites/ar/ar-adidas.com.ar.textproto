      control_type: INPUT
      concepts: "unset"
      autocomplete_attribute: "home tel"
      section: PHONE
    }
    fields {
      name: "billingAddress-documentValue|additionalBillingAddress.documentValue"
      label: "DNI (ej. 12345678)"
      label_translated: "DNI (ex. 12345678)"
      control_type: INPUT
      concepts: "unset"
      section: OTHER
    }
    fields {
      hide: true
      name: "|shouldUseShippingAddressForBilling"
      control_type: INPUT
      concepts: "unset"
      section: ADDRESS
    }
    fields {
      hide: true
      name: "|"
