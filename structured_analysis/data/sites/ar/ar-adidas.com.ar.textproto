      name: "|persistentLogin"
      label: "Mantener sesión abierta.Más información"
      label_translated: "Keep session open.Learn more"
      control_type: INPUT
      concepts: "unset"
      section: LOGIN
    }
    fields {
      name: "coupon-input|coupon"
      label: "Introduce tu código promocional"
      label_translated: "Enter your promotional code"
      control_type: INPUT
      concepts: "unset"
