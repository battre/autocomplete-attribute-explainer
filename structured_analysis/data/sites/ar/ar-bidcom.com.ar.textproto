site_examples {
  url: "	https://www.bidcom.com.ar"
  is_structured: true
  locale {
    country: "AR"
    language: "es"
  }
  sequences {
    fields {
      name: "fecha_entrega|fecha_entrega"
      control_type: INPUT
      concepts: "unset"
      section: OTHER
    }
    fields {
      name: "|id_direccion"
      label: "Paso 1"
      label_translated: "Step 1"
      control_type: INPUT
      concepts: "unset"
      section: OTHER
    }
    fields {
      name: "numero|numero"
      label: "Código postal *"
      label_translated: "Zip Code * "
      control_type: INPUT
      concepts: "postal-code"
      autocomplete_attribute: "off"
      section: ADDRESS
    }
    fields {
      name: "|nombre_ciudad"
      label: "Localidad *"
      label_translated: "Location *"
      control_type: SELECT
      concepts: "address-level2"
      section: ADDRESS
    }
    fields {
      name: "calle|calle"
      label: "Calle *"
      label_translated: "Street *"
      example: "Ej. Avenida San Martín"
      example_translated: "Ex. San Martin Avenue"
      control_type: INPUT
      concepts: "street-name"
      autocomplete_attribute: "off"
      section: ADDRESS
    }
    fields {
      name: "numero|numero"
      label: "Altura *"
      label_translated: "Height *"
