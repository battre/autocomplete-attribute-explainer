      example: "Numero"
      example_translated: "Number"
      control_type: INPUT
      concepts: "house-number"
      autocomplete_attribute: "off"
      section: ADDRESS
    }
    fields {
      name: "piso|piso"
      label: "Piso / Departamento"
      label_translated: "Floor / Apartment"
      example: "Ej. 2 A"
      example_translated: "Ex. 2 A"
      control_type: INPUT
      # concepts: "floor"
      # concepts: "apartment"
      concepts: "location-in-building"
      autocomplete_attribute: "off"
      section: ADDRESS
    }
    fields {
      name: "entrecalle1|entre_calle1"
      label: "Entrecalles: Calle 1*"
      label_translated: "Between streets: Street 1*"
      control_type: INPUT
      concepts: "gap"
      autocomplete_attribute: "off"
      section: ADDRESS
    }
    fields {
      name: "entrecalle2|entre_calle2"
      label: "Entrecalles: Calle 2"
      label_translated: "Between streets: street 2"
      control_type: INPUT
      concepts: "gap"
      autocomplete_attribute: "off"
      section: ADDRESS
    }
    fields {
      name: "|telefono"
      label: "0"
      label_translated: "0"
      control_type: INPUT
      concepts: "unset"
      autocomplete_attribute: "off"
      section: PHONE
    }
    fields {
      name: "input-button|"
      control_type: INPUT
      concepts: "unset"
      section: OTHER
    }
